  "name": "Inventorymanager",
  "description": "A Minecraft Fabric client-side mod to revolutionize the inventory.",
  "authors": [
    "WNTIV"
  ],
  "contact": {
    "repo": "https://github.com/wntiv-main/Inventory-Manager"
  },
  "license": "GPL-3.0",
  "icon": "assets/inventorymanager/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.wntiv.inventorymanager.client.InventoryManagerClient"
    ],
    "main": [
      "net.wntiv.inventorymanager.InventoryManager"
    ]
  },
  "mixins": [
    "inventorymanager.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.9",
